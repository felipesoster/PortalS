
@{
    ViewBag.Title = "CadastroEntidade";
}

<h2>Cadastro Entidade</h2>

<div class="container">
    <form action="/Home/FinalizarCadastro">
        <label for="nome">Nome:</label>
        <input id="nome" name="nome" class="form-control" />

        <label for="cnpj">CNPJ:</label>
        <input id="cnpj" name="cpf" class="form-control" />

        <label for="email">Email:</label>
        <input id="email" name="email" class="form-control" />

        <label for="endereco">Endereço:</label>
        <input id="endereco" name="endereco" class="form-control" />

        <label for="senha">Senha:</label>
        <input id="senha" name="senha" class="form-control" />

        <input type="submit" class="form-control" value="Finalizar Cadastro"/>
    </form>
</div>